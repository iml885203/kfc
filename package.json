{
  "name": "@logan/kfc",
  "version": "0.1.0",
  "description": "Kubernetes Follow Colorful - Follow Kubernetes deployment logs with style",
  "type": "module",
  "bin": {
    "kfc": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.tsx",
    "start": "node dist/cli.js",
    "prepublishOnly": "npm run build",
    "test": "npm run build && node dist/cli.js --version"
  },
  "keywords": [
    "kubernetes",
    "k8s",
    "logs",
    "kubectl",
    "cli",
    "ink",
    "typescript"
  ],
  "author": "logan",
  "license": "MIT",
  "files": [
    "dist/",
    "README.md"
  ],
  "engines": {
    "node": ">=14"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "main": "index.js",
  "dependencies": {
    "@kubernetes/client-node": "^1.4.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "execa": "^9.6.1",
    "ink": "^6.6.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "meow": "^14.0.0",
    "react": "^19.2.4"
  },
  "devDependencies": {
    "@types/node": "^25.1.0",
    "@types/react": "^19.2.10",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
